{
    "name": "@doist/reactist",
    "description": "Open source React components by Doist",
    "author": "Henning Muszynski <henning@doist.com> (http://doist.com)",
    "version": "5.2.0",
    "license": "MIT",
    "homepage": "https://github.com/Doist/reactist#readme",
    "repository": "git+https://github.com/Doist/reactist.git",
    "prettier": "@doist/prettier-config",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "npm run build-tsdx && npm run build-webpack; rimraf 'dist/umd'",
        "build-webpack": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color",
        "build-tsdx": "tsdx build --tsconfig tsconfig.dist.json",
        "build-watch": "tsdx watch",
        "build-types": "rimraf 'lib/**/*.d.ts'; npx tsc -p tsconfig.lib.json;",
        "build-storybook": "build-storybook -o docs",
        "build-all": "npm run build && npm run build-storybook",
        "test": "tsdx test --passWithNoTests",
        "type-check": "tsc --noEmit -p ./tsconfig.json",
        "lint": "tsdx lint src stories",
        "storybook": "start-storybook -p 6006",
        "prettify": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\""
    },
    "peerDependencies": {
        "classnames": "^2.2.5",
        "dayjs": "^1.8.10",
        "react": "^15.5.4 || ^16.2.0",
        "react-dom": "^15.5.4 || ^16.2.0",
        "prop-types": "^15.6.2"
    },
    "module": "dist/index/index.js",
    "devDependencies": {
        "@babel/core": "^7.9.6",
